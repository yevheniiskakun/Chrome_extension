{
    "manifest_version": 2,
    "name": "Minecraft Weather",
    "version": "1.0.2",

    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],
      "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
  }
